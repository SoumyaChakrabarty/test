// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a[5],b[5],i,sum[5];
    printf("Enter 5 elements for the first aray: ");
    for(i=0;i<5;i++){
        scanf("%d",&a[i]);
    }
    printf("Enter 5 elements for the second aray: ");
    for(i=0;i<5;i++){
        scanf("%d",&b[i]);
    }
    printf("The sum of the corresponding elements of tboth thr arrays is: ");
    for(i=0;i<5;i++){
        sum[i] = a[i]+b[i];
    }
    for(i=0;i<5;i++){
        printf("%d ",sum[i]);
    }
    return 0;
}
